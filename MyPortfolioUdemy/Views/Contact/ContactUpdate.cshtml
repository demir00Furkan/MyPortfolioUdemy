@model Contact
@{
    ViewData["Title"] = "ContactUpdate";
    Layout = "~/Views/Layout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">İletişim</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-header">
						<div class="card-title">İletişim Güncelleme Sayfası</div>
					</div>
					<div class="card-body">
						<form method="post">
							<input type="hidden" asp-for="ContactId" />
							<label>Ünvan </label>
							<input type="text" asp-for="Title" class="form-control" />
							<br />
							<label>Detay </label>
							<input type="text" asp-for="Description" class="form-control" />
							<br />
							<label>Telefon </label>
							<input type="text" asp-for="Phone2" class="form-control" />
							<br />
							<label>Email 1 </label>
							<input type="text" asp-for="Email1" class="form-control" />
							<br />
							<label> Email 2</label>
							<input type="text" asp-for="Email2" class="form-control" />
							<br />
							<label>Address</label>
							<input type="text" asp-for="Address" class="form-control" />
							<br />
							<button class="btn btn-warning">Değişiklikleri Kaydet</button>
						</form>

					</div>
				</div>
			</div>
		</div>
	</div>
</div>



